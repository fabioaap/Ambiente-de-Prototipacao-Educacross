{
  "$schema": "https://www.designtokens.org/schemas/2025-10/token.json",
  "color": {
    "brand": {
      "primary": {
        "$value": "#5B5BD6",
        "$type": "color"
      }
    },
    "text": {
      "default": {
        "$value": "{color.brand.primary}",
        "$type": "color"
      }
    }
  },
  "size": {
    "radius": {
      "lg": {
        "$value": "12px",
        "$type": "dimension"
      }
    },
    "spacing": {
      "md": {
        "$value": "16px",
        "$type": "dimension"
      }
    }
  }
}